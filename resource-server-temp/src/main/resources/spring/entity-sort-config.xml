		<bean class="ua.southpost.resource.commons.model.dto.SortOption">
			<constructor-arg index="0" value="street.settlement.district.region.searchValue"/>
			<constructor-arg index="1" value="ASC"/>
		</bean>
		<bean class="ua.southpost.resource.commons.model.dto.SortOption">
			<constructor-arg index="0" value="street.settlement.district.searchValue"/>
			<constructor-arg index="1" value="ASC"/>
